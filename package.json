{
  "name": "@eventop/sdk",
  "version": "1.2.15",
  "description": "AI-powered guided tours for any web app. Drop-in, themeable, provider-agnostic.",
  "keywords": [
    "onboarding",
    "tour",
    "guided-tour",
    "ai",
    "react"
  ],
  "homepage": "https://github.com/eventop-s/sdk#readme",
  "bugs": {
    "url": "https://github.com/eventop-s/sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eventop-s/sdk.git"
  },
  "license": "AGPL-3.0-only",
  "author": "Emengkeng Juslen",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react/index.js",
      "require": "./dist/react/index.cjs",
      "types": "./dist/react/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "yarn run build"
  },
  "dependencies": {
    "node-fetch": "^2.6.7",
    "react": "^19.2.4"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/plugin-transform-react-jsx": "^7.28.6",
    "@babel/preset-env": "^7.29.0",
    "@babel/preset-react": "^7.23.3",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "rollup": "^4.9.6",
    "typescript": "^5.9.3"
  }
}
